{
  "manifest_version": 2,
  "name": "ForecastGenie",
  "short_name": "ForecastGenie",
  "author": "Max Helmetag",
  "version": "1.0.0",
  "version_name": "1.0.0",
  "description": "Reveal the 3rd, 4th and 5th Day Forecasts on Surfline",
  "content_scripts": [
    {
      "matches": [
        "https://www.surfline.com/surf-forecasts/*"
      ],
      "js": [
        "build/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "build/script.js"
  ]
}
